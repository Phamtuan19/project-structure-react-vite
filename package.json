{
   "name": "project-structure",
   "private": true,
   "version": "0.0.0",
   "type": "module",
   "scripts": {
      "dev": "vite",
      "build": "vite build",
      "build:staging": "tsc && vite build --mode staging",
      "build:production": "tsc && vite build --mode production",
      "commitlint": "commitlint --edit",
      "staging": "vite --mode staging --port 3002",
      "production": "vite --mode production --port 3003",
      "format:check": "prettier . --check",
      "format:fix": "prettier . --write",
      "format:svg": "svgo . -r --config svgo.config.cjs",
      "lint": "eslint src --max-warnings 0 --report-unused-disable-directives",
      "lint:fix": "eslint src --fix --max-warnings 0 --report-unused-disable-directives",
      "prepare": "husky install",
      "preview": "vite preview",
      "type:check": "tsc --noEmit",
      "setup": "git init && husky install",
      "validate": "run-p lint type:check",
      "test": "vitest",
      "test:ui": "vitest --ui",
      "test:run": "vitest run",
      "test:coverage": "vitest run --coverage",
      "check-upgrade": "node scripts/check-upgrade-progress.cjs",
      "vite": "vite"
   },
   "dependencies": {
      "@ant-design/icons": "^6.0.0",
      "@hookform/resolvers": "^5.2.2",
      "@loadable/component": "^5.16.4",
      "@tailwindcss/vite": "^4.1.17",
      "@tanstack/react-query": "^5.90.10",
      "antd": "^5.29.1",
      "autoprefixer": "^10.4.22",
      "axios": "^1.13.2",
      "clsx": "^2.1.1",
      "file-saver": "^2.0.5",
      "i18next": "^25.6.3",
      "i18next-typescript": "^0.1.0",
      "immer": "^10.1.1",
      "p-min-delay": "^4.1.0",
      "postcss": "^8.5.3",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-hook-form": "^7.66.1",
      "react-i18next": "^16.3.4",
      "react-icons": "^5.5.0",
      "react-intersection-observer": "^10.0.0",
      "react-router-dom": "^7.12.0",
      "sass": "^1.94.1",
      "svgo-config": "^0.10.0",
      "tailwind-merge": "^3.4.0",
      "tailwindcss": "^4.1.17",
      "zod": "^4.1.12",
      "zustand": "^5.0.4"
   },
   "devDependencies": {
      "@commitlint/cli": "^20.1.0",
      "@commitlint/config-conventional": "^20.0.0",
      "@rollup/plugin-strip": "^3.0.4",
      "@testing-library/dom": "^10.4.1",
      "@testing-library/jest-dom": "^6.9.1",
      "@testing-library/react": "^16.3.1",
      "@testing-library/user-event": "^14.6.1",
      "@total-typescript/ts-reset": "^0.6.1",
      "@types/axios": "^0.14.4",
      "@types/file-saver": "^2.0.7",
      "@types/loadable__component": "^5.13.9",
      "@types/node": "20.11.30",
      "@types/react": "18.2.66",
      "@types/react-dom": "18.2.22",
      "@types/react-i18next": "^8.1.0",
      "@typescript-eslint/eslint-plugin": "^8.47.0",
      "@typescript-eslint/parser": "^8.47.0",
      "@vitejs/plugin-react-swc": "^4.2.2",
      "eslint": "^8.57.0",
      "eslint-config-prettier": "^10.1.8",
      "eslint-import-resolver-typescript": "^4.4.4",
      "eslint-plugin-filename-rules": "^1.0.0",
      "eslint-plugin-import": "^2.29.1",
      "eslint-plugin-jsdoc": "^61.2.1",
      "eslint-plugin-jsx-a11y": "^6.8.0",
      "eslint-plugin-react": "^7.34.1",
      "eslint-plugin-react-hooks": "^7.0.1",
      "eslint-plugin-react-refresh": "^0.4.3",
      "eslint-plugin-security": "^1.7.1",
      "eslint-plugin-tsdoc": "^0.5.0",
      "globals": "^16.5.0",
      "husky": "^9.1.7",
      "jsdom": "^27.4.0",
      "lint-staged": "^16.2.6",
      "npm-run-all": "^4.1.5",
      "prettier": "^3.5.3",
      "prettier-plugin-sort-json": "^4.1.1",
      "prettier-plugin-tailwindcss": "^0.7.1",
      "terser": "^5.44.1",
      "typescript": "~5.9.3",
      "vite": "^7.2.2",
      "vite-plugin-checker": "^0.11.0",
      "vite-plugin-compression": "^0.5.1",
      "vite-plugin-dynamic-import": "^1.6.0",
      "vite-plugin-svgr": "^4.3.0",
      "vite-tsconfig-paths": "^5.1.4",
      "vitest": "^4.0.16"
   }
}
